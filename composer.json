{
    "name": "soap/yii2-vimeo-player",
    "description": "Yii2 widget to display vimeo video player",
    "type": "yii2-extension",
    "license": "GPLV3",
    "authors": [
        {
            "name": "Prasit Gebsaap",
            "email": "prasit.gebsaap@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0",
        "bower-asset/vimeo-player-js": "^2.11"   
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "soap\\VimeoPlayer\\": "src"
        }
      },
      "extra": {
        "asset-installer-paths": {
          "npm-asset-library": "vendor/npm",
          "bower-asset-library": "vendor/bower"
        }
      }
}
